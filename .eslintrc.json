//Inf obout Eslint: https://lenguajejs.com/javascript/caracteristicas/eslint/
{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "prefer-destructuring": 0,
        "import/extensions": 0,
        "import/prefer-default-export": 0
    }
}
